                    "value": "${context.startDate}",
                    "constraints": {
                        "required": false
                    }
                },
                {
                    "id": "end_date",
                    "type": "string",
                    "label": "End Date",
                    "readOnly": true,
                    "value": "${context.endDate}",
                    "constraints": {
                        "required": false
                    }
                },
                {
                    "id": "reporting_year",
                    "type": "string",
