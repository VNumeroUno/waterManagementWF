                    "constraints": {
                        "required": false
                    }
                },
                {
                    "id": "approvers",
                    "type": "string",
                    "label": "Approvers",
                    "readOnly": false,
                    "value": "${context.approvers}",
                    "constraints": {
                        "required": false
                    }
                },
                {
                    "id": "comments",
                    "type": "string",
                    "label": "Comments",
                    "readOnly": false,
                    "value": "${context.Comments}",
                    "constraints": {
                        "required": false
                    }
                }
